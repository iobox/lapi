{
  "name": "lapi",
  "version": "0.1.3",
  "description": "Light API framework for NodeJS",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/gulp test",
    "dev": "./node_modules/.bin/gulp watch:test",
    "build": "./node_modules/.bin/gulp build",
    "doc": "./node_modules/.bin/esdoc -c esdoc.json",
    "prestart": "npm run doc",
    "start": "SERVER_ROOT=./docs/api node server.js",
    "indexing": "./node_modules/.bin/gulp indexing",
    "deploy": "npm run build && npm run indexing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iobox/lapi.git"
  },
  "keywords": [
    "restful",
    "api",
    "framework",
    "node",
    "nodejs",
    "lapi",
    "iobox"
  ],
  "author": {
    "name": "Long Do",
    "email": "me@dotronglong.com",
    "url": "http://dotronglong.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iobox/lapi/issues"
  },
  "homepage": "https://github.com/iobox/lapi#readme",
  "engines": {
    "node": ">=5.1.0"
  },
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "gulp-jsdoc3": "^0.3.0",
    "lapi-dev": "^1.0.0"
  },
  "dependencies": {
    "esdoc": "^0.4.8",
    "async": "^2.0.0",
    "path": "^0.12.7"
  }
}
